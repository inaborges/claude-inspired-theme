    "gitDecoration.ignoredResourceForeground": "#8B8583",
    "gitDecoration.conflictingResourceForeground": "#B87BA8"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8B8583",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#C96846"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#52A66E"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#B87BA8"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#2C2A29"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#5B6B7A"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#5B9BD5"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class"],
      "settings": {
        "foreground": "#D9A357"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#D97757"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#5B9BD5"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#5B6B7A"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#6B6967"
      }
    },
    {
      "scope": ["meta.brace", "punctuation.section"],
      "settings": {
        "foreground": "#6B6967"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#D94040"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#D97757",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#5B9BD5"
      }
    }
  ]
}
