  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8B8583",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#E88567"
      }
    },
    {
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#FFA07A"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#7CD098"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#CD90BD"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#E8E6E3"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#B5C4D3"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#85C3FF"
      }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class"],
      "settings": {
        "foreground": "#FFC77C"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#E88567"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#85C3FF"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#B5C4D3"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#A5A3A0"
      }
    },
    {
      "scope": ["meta.brace", "punctuation.section"],
      "settings": {
        "foreground": "#A5A3A0"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#FF6A6A"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#E88567",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#85C3FF"
      }
    }
  ]
}
